{
  "name": "nn",
  "version": "0.0.4",
  "description": "Node.js version manager",
  "main": "lib",
  "bin": {
    "nn": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "build": "rm -rf dist && pkg --out-path dist ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pd4d10/nn.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pd4d10/nn/issues"
  },
  "homepage": "https://github.com/pd4d10/nn",
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.16.0",
    "fs-extra": "^7.0.0",
    "fstream": "^1.0.11",
    "node-fetch": "^2.1.2",
    "progress": "^2.0.0",
    "request": "^2.87.0",
    "rimraf": "^2.6.2",
    "targz": "^1.0.1",
    "unzipper": "^0.9.2"
  },
  "devDependencies": {
    "jest": "^23.4.1"
  },
  "pkg": {
    "targets": [
      "win-x64",
      "win-x86",
      "macos-x64",
      "linux-x64"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "test/*.test.js",
        "options": {
          "printWidth": 150
        }
      }
    ]
  }
}
